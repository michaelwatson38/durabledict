{
  "arcanist_configuration": "DisqusConfiguration",
  "base": "git:merge-base(origin/master), arc:upstream, git:HEAD^",
  "conduit_uri": "http://phabricator.local.disqus.net/",
  "copyright_holder": "Disqus, Inc.",
  "coverage": false,
  "differential.field-selector": "DisqusDifferentialFieldSelector",
  "immutable_history": false,
  "lint.flake8.options": "--ignore=W391,W292,W293,E501,E225,E121,E123,E124,E127,E128,F999,F821 --exclude='*/contrib/*'",
  "lint.pep8.options": "--ignore=W391,W292,W293,E501,E225,W602 --exclude='*/contrib/*'",
  "lint_engine": "DisqusLintEngine",
  "load": ["libdisqus/src"],
  "project_id": "durabledict",
  "unit.engine.setup_py_args": "nosetests --with-xunit --xunit-file=test_results/nosetests.xml",
  "unit_engine": "SetupPyUnitTestEngine"
}
